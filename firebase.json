{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/router/**",
        "destination": "/router/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
